<div class='col-md-6'>
  <h3><%= set.capitalize %></h3>

  <div class='<%= set %> list-group'>
    <% results.each do |item| %>
        <a href="<%= company_path(item.id) %>" class='list-group-item media'>
          <div class="pull-right">
            <%= crunchbase_image item.image %>
          </div>
          <div>
            <h5 class='name '><%= item.name %></h5>
            <div class='description'><%= item.description %></div>
          </div>
        </a>
    <% end %>
  </div>
</div>